source rr/breakpoints.py

set pagination off
set logging file rr/breakpoints.log
set logging overwrite on
set logging redirect on
set logging on

p $init_argument()

while 1 == 1
    p $update_file()
    c
    p $run_break_commands()
    if $is_process_exit() == 1
        loop_break
    end
end

set logging off
quit
