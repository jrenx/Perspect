source rr/watchpoints.py

set pagination off
set logging file rr/watchpoints.log
set logging overwrite on
set logging redirect on
set logging on

p $init_argument()

while 1 == 1
    p $update_file()
    c
    if $is_process_exit() == 1
        loop_break
    end
    i reg pc
end

set logging off
quit
