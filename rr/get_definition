source result_parser.py

set pagination off

p $init_argument()
set logging file result.log
set logging overwrite on
set logging redirect on
set logging on
p $set_breakpoint()
set logging off

if $is_br_success() == 0
    echo Setting breakpoint failed
    quit
end

r

set logging file result.log
set logging overwrite on
set logging redirect on
set logging on
set $continue_count = 1
while 1 == 1
    p $update_file()
    c $continue_count

    if $is_process_exit() == 1
        echo end of program detected
        loop_break
    end

    while 1 == 1
        p $update_file()
        reverse_cont
        if $is_program_stop() == 1:
            loop_break
        end
    end

    set $continue_count = $continue_count + 1

end
set logging off
set pagination on
quit