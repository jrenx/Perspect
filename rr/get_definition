source result_parser.py

p $init_argument()
set logging file result.log
set logging overwrite on
set logging redirect on
set logging on
p $set_breakpoint()
set logging off

if $is_br_success() == 0
    echo Setting breakpoint failed
    quit
end

r

set $continue_count = 1
while 1
    set logging file result.log
    set logging overwrite on
    set logging redirect on
    set logging on
    c $continue_count
    set logging off

quit
    if $is_process_exit() == 1
      quit
    end

end