# observe the baseline:
rm -rf db
mkdir db
./run.sh
#in another window or terminal, run
./mongo setup.js # creates the database 
./mongo fast.js # reproduce the fast run

# observe the slowdown:
rm -rf db
mkdir db
./run.sh
#in another window or terminal, run
./mongo setup.js # creates the database 
./mongo slow.js # reproduce the slow run
