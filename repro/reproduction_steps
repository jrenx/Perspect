# observe the baseline:
rm -rf db
mkdir db
./4.0.13/mongod_4.0.13 --dbpath ./db  --wiredTigerCacheSizeGB 1  > log
#in another window or terminal, run
python3 createDB.py # creates the database 
#if you get an error saying you don't have pymongo, install pymongo
pip3 install pymongo
python3 repro.py # reproduce the error

# observe the slowdown:
rm -rf db
mkdir db
./4.2.1/mongod_4.2.1 --dbpath ./db  --wiredTigerCacheSizeGB 1  > log
#in another window or terminal, run
python3 createDB.py 
python3 repro.py 


